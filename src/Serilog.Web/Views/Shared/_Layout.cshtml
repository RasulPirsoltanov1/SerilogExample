                </a>
            </div>
            <div>
                @if (!User.Identity.IsAuthenticated)
                {
                    <a class="py-2 d-none d-md-inline-block me-lg-4" asp-controller="Auth" asp-action="Register">Register</a>
                    <a class="py-2 d-none d-md-inline-block me-lg-4" asp-controller="Auth" asp-action="Login">Login</a>
                }
                else
                {
                    <a class="py-2 d-none d-md-inline-block" asp-controller="Auth" asp-action="Logout" asp-route-returnUrl="@Context.Request.Path">Log Out</a>
                }

            </div>
        </nav>
